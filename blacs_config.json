{
  "system": {
    "name": "BLACS Guardian",
    "version": "2.0.0",
    "description": "Advanced Anti-Cheat System with DSLL Technology and Comprehensive Logging",
    "admin_required": true,
    "self_protection": false,
    "tamper_resistant": true,
    "bsod_protection": false,
    "safe_mode": true
  },
  "protection_levels": {
    "safe": {
      "description": "Ultra-safe protection that only blocks obvious cheat tools",
      "max_human_frequency": 100.0,
      "automation_threshold": 0.95,
      "auto_terminate": false,
      "extreme_detection": false,
      "dsll_enabled": true,
      "scan_interval": 5.0,
      "critical_risk_threshold": 0.99
    },
    "low": {
      "description": "Basic protection for development and testing",
      "max_human_frequency": 50.0,
      "automation_threshold": 0.9,
      "auto_terminate": false,
      "extreme_detection": false,
      "dsll_enabled": false,
      "scan_interval": 5.0,
      "critical_risk_threshold": 0.98
    },
    "medium": {
      "description": "Balanced protection for general use",
      "max_human_frequency": 25.0,
      "automation_threshold": 0.8,
      "auto_terminate": false,
      "extreme_detection": false,
      "dsll_enabled": true,
      "scan_interval": 3.0,
      "critical_risk_threshold": 0.95
    },
    "high": {
      "description": "Strict protection for important applications",
      "max_human_frequency": 15.0,
      "automation_threshold": 0.7,
      "auto_terminate": true,
      "extreme_detection": true,
      "dsll_enabled": true,
      "scan_interval": 2.0,
      "critical_risk_threshold": 0.9
    },
    "maximum": {
      "description": "Extreme protection for critical applications",
      "max_human_frequency": 10.0,
      "automation_threshold": 0.6,
      "auto_terminate": true,
      "extreme_detection": true,
      "dsll_enabled": true,
      "scan_interval": 1.0,
      "critical_risk_threshold": 0.85
    }
  },
  "monitors": {
    "input_monitor": {
      "enabled": true,
      "description": "Monitors input patterns for automation detection",
      "settings": {
        "click_frequency_threshold": 20.0,
        "key_frequency_threshold": 30.0,
        "pattern_analysis_window": 100,
        "suspicious_pattern_threshold": 0.7,
        "target_process_only": true
      }
    },
    "memory_monitor": {
      "enabled": true,
      "description": "Monitors memory access and modification attempts",
      "settings": {
        "scan_interval": 1.0,
        "protected_regions": ["heap", "stack", "code"],
        "detection_methods": ["signature", "heuristic", "behavioral"],
        "memory_protection_level": "high",
        "target_process_only": true,
        "monitor_external_access": true
      }
    },
    "process_monitor": {
      "enabled": true,
      "description": "Monitors processes for cheat tools and suspicious behavior",
      "settings": {
        "scan_interval": 2.0,
        "signature_database_size": 500,
        "behavioral_analysis": false,
        "auto_terminate_threats": true,
        "whitelist_system_processes": true,
        "scan_mode": "malicious_only",
        "ignore_system_processes": true,
        "focus_on_target": true
      }
    },
    "dsll_monitor": {
      "enabled": true,
      "description": "Advanced DSLL syscall monitoring and behavioral analysis",
      "settings": {
        "monitor_interval": 0.1,
        "ledger_max_size": 10000,
        "pattern_analysis_window": 50,
        "cryptographic_verification": true,
        "auto_export_on_violation": false,
        "export_format": "json",
        "target_process_only": true,
        "monitor_external_interactions": true
      }
    }
  },
  "dsll_configuration": {
    "enabled": true,
    "description": "Deterministic Syscall Lockstep Ledger Technology",
    "critical_syscalls": [
      "NtReadVirtualMemory",
      "NtWriteVirtualMemory",
      "NtOpenProcess",
      "NtCreateThread",
      "NtSuspendProcess",
      "NtResumeProcess",
      "NtTerminateProcess",
      "NtAllocateVirtualMemory",
      "NtProtectVirtualMemory",
      "NtCreateFile",
      "NtSetValueKey",
      "NtLoadDriver",
      "NtCreateThreadEx",
      "NtQueueApcThread",
      "NtSetContextThread"
    ],
    "pattern_detection": {
      "rapid_memory_threshold": 10,
      "process_manipulation_threshold": 5,
      "injection_attempt_threshold": 3,
      "analysis_window_size": 50,
      "pattern_confidence_threshold": 0.8
    },
    "forensic_settings": {
      "enable_stack_traces": true,
      "enable_parameter_logging": true,
      "enable_return_value_logging": true,
      "compression_enabled": false,
      "encryption_enabled": false,
      "max_log_file_size_mb": 100
    }
  },
  "threat_detection": {
    "signature_database": {
      "memory_editors": [
        "cheatengine.exe", "cheatengine-x86_64.exe", "cheatengine-i386.exe",
        "artmoney.exe", "artmoneypro.exe", "artmoney64.exe",
        "gameguardian.exe", "memoryeditor.exe", "memoryhacker.exe", 
        "memhack.exe", "tsearch.exe", "scanmem.exe", "memoryviewer.exe"
      ],
      "debuggers": [
        "ollydbg.exe", "x64dbg.exe", "x32dbg.exe",
        "processhacker.exe", "processhacker2.exe",
        "ida.exe", "ida64.exe", "idapro.exe", "windbg.exe"
      ],
      "injection_tools": [
        "injector.exe", "dllinjector.exe", "processinjector.exe",
        "codecave.exe", "hooklib.exe", "apihook.exe"
      ],
      "speed_hacks": [
        "speedhack.exe", "gamespeed.exe", "timescale.exe",
        "clockblocker.exe", "speedgear.exe", "hourglass.exe"
      ],
      "trainers": [
        "trainer.exe", "gametrainer.exe", "fling.exe",
        "mrantifun.exe", "wemod.exe", "plitch.exe"
      ],
      "automation_bots": [
        "autoclicker.exe", "autoclick.exe", "clickbot.exe",
        "mousebot.exe", "keybot.exe", "inputbot.exe",
        "autohotkey.exe", "ahk.exe", "aimbot.exe"
      ]
    },
    "termination_policy": {
      "terminate_only_exact_matches": true,
      "log_all_processes": true,
      "ignore_system_services": true,
      "ignore_windows_services": true,
      "ignore_audio_services": true,
      "ignore_defender_services": true
    },
    "behavioral_patterns": {
      "external_memory_access": {
        "enabled": true,
        "monitor_read_attempts": true,
        "monitor_write_attempts": true,
        "monitor_process_access": true,
        "confidence_level": 0.95,
        "action": "log_only"
      }
    }
  },
  "response_actions": {
    "violation_handling": {
      "low_severity": {
        "log_event": true,
        "notify_user": false,
        "terminate_threat": false,
        "export_evidence": false
      },
      "medium_severity": {
        "log_event": true,
        "notify_user": true,
        "terminate_threat": false,
        "export_evidence": true
      },
      "high_severity": {
        "log_event": true,
        "notify_user": true,
        "terminate_threat": true,
        "export_evidence": true
      },
      "critical_severity": {
        "log_event": true,
        "notify_user": true,
        "terminate_threat": true,
        "export_evidence": true,
        "immediate_response": true
      }
    },
    "termination_methods": [
      "terminate",
      "kill",
      "taskkill"
    ],
    "notification_settings": {
      "console_output": true,
      "log_file": true,
      "system_tray": false,
      "email_alerts": false
    }
  },
  "logging": {
    "enabled": true,
    "log_level": "INFO",
    "log_file": "blacs_guardian.log",
    "max_log_size_mb": 100,
    "log_rotation": true,
    "log_retention_days": 30,
    "detailed_logging": {
      "all_processes": true,
      "system_processes": true,
      "process_creation": true,
      "process_termination": true,
      "memory_access_attempts": true,
      "syscalls": true,
      "process_events": true,
      "memory_events": true,
      "input_events": true,
      "performance_metrics": true,
      "threat_detections": true,
      "false_positives": true
    },
    "log_format": {
      "timestamp": true,
      "process_id": true,
      "process_name": true,
      "process_path": true,
      "parent_process": true,
      "command_line": true,
      "user_context": true,
      "threat_level": true,
      "action_taken": true
    }
  },
  "performance": {
    "max_cpu_usage_percent": 5.0,
    "max_memory_usage_mb": 100,
    "optimization_level": "balanced",
    "background_priority": "high",
    "thread_pool_size": 4,
    "cache_size_mb": 20
  },
  "advanced_settings": {
    "system_protection": {
      "enabled": true,
      "ignore_system_processes": true,
      "ignore_windows_processes": true,
      "ignore_microsoft_processes": true,
      "system_process_patterns": [
        "system", "smss.exe", "csrss.exe", "wininit.exe", "winlogon.exe",
        "services.exe", "lsass.exe", "svchost.exe", "explorer.exe",
        "dwm.exe", "conhost.exe", "audiodg.exe", "spoolsv.exe",
        "taskmgr.exe", "taskhost.exe", "taskhostw.exe"
      ],
      "scan_mode": "targeted_only",
      "focus_on_malicious": true
    },
    "custom_signatures": {
      "enabled": true,
      "user_defined_patterns": [],
      "scan_only_malicious": true,
      "ignore_legitimate_software": true
    },
    "experimental_features": {
      "machine_learning": false,
      "cloud_intelligence": false,
      "predictive_analysis": false,
      "quantum_resistance": false
    },
    "developer_options": {
      "debug_mode": false,
      "verbose_logging": false,
      "performance_profiling": false,
      "api_access": false
    }
  }
}